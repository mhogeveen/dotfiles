{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "console_title_template": "{{ .Shell }} in {{ .Folder }}",
  "blocks": [
    {
      "alignment": "left",
      "filler": "-",
      "segments": [
        {
          "foreground": "black",
          "background": "blue",
          "style": "accordion",
          "template": " mjh ",
          "type": "session"
        },
        {
          "type": "path",
          "style": "plain",
          "foreground": "black",
          "background": "green",
          "template": " {{ .Path }} ",
          "properties": {
            "style": "agnoster"
          }
        },
        {
          "foreground": "black",
          "background": "yellow",
          "properties": {
            "branch_icon": ""
          },
          "style": "plain",
          "template": " {{ .HEAD }} ",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "green",
          "style": "plain",
          "template": "  ",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "transient_prompt": {
    "foreground": "white",
    "template": "  "
  },
  "version": 2
}
