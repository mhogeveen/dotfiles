{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "console_title_template": "{{ .Shell }} in {{ .Folder }}",
  "blocks": [
    {
      "alignment": "left",
      "filler": "-",
      "segments": [
        {
          "foreground": "blue",
          "style": "accordion",
          "template": "⦋ Maarten ⦌",
          "type": "session"
        },
        {
          "type": "path",
          "style": "plain",
          "foreground": "cyan",
          "template": "<darkGray>-</>⦋ {{ .Path }} ⦌",
          "properties": {
            "style": "agnoster"
          }
        },
        {
          "foreground": "green",
          "properties": {
            "branch_icon": ""
          },
          "style": "plain",
          "template": "<darkGray>-</>⦋ {{ .HEAD }} ⦌",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "type": "php",
          "style": "plain",
          "foreground": "magenta",
          "template": "⦋ PHP: {{ .Full }} ⦌<darkGray>-</>",
          "properties": {
            "display_mode": "files"
          }
        },
        {
          "type": "node",
          "style": "plain",
          "foreground": "red",
          "template": "⦋ NODE: {{ .Full }} ⦌<darkGray>-</>"
        },
        {
          "type": "executiontime",
          "style": "plain",
          "foreground": "cyan",
          "template": "⦋ {{ .FormattedMs }} ⦌",
          "properties": {
            "threshold": 0,
            "style": "roundrock",
            "always_enabled": true
          }
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "green",
          "style": "plain",
          "template": "   ",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "transient_prompt": {
    "foreground": "green",
    "template": "   "
  },
  "version": 2
}
